<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<sectionGroup name="ZPW.All.framework.metaConfig" type="ZPW.Util.Configuration.SectionGroup.MetaConfigurationSectionGroup, ZPW.Util">
			<section name="sourceMappings" type="ZPW.Util.Configuration.Section.MetaSourceMappingsConfigurationSection, ZPW.Util"/>
		</sectionGroup>
	</configSections>

	<ZPW.All.framework.metaConfig>
		<sourceMappings>
			<instances>
				<!--path配置路径是基于 applications中指定的app路径相对路径-->
				<add name="默认网站" path="ZPW.Framework.config" mode="win">
					<applications>
						<add name="" app="/" />
					</applications>
				</add>
			</instances>
		</sourceMappings>
	</ZPW.All.framework.metaConfig>
</configuration>